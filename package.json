  "scripts": {
    "clean": "rm -rf ./dist/*",
    "prebuild": "npm run clean && cp -rf ./src/static/. ./dist/",
    "build": "webpack --mode production",
    "start": "node ./dist/server.js",
    "watch:build": "webpack --mode development --watch",
    "watch:start": "until [ -f ./dist/server.js ]; do sleep 1; done && nodemon --watch ./dist ./dist/server.js",
    "start:dev": "npm run prebuild && npm-run-all --parallel watch:*",
    "setup": "if [ ! -f private/.env ]; then mkdir -p private && cp example.env private/.env; fi"
  },
